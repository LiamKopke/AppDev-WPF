    <Color x:Key="RedGradient1">#F08080</Color>
    <Color x:Key="RedGradient2">#F27370</Color>
    <Color x:Key="RedGradient3">#F46058</Color>
    <Color x:Key="RedGradient4">#F55348</Color>
    <Color x:Key="RedGradient5">#F54C40</Color>
    <Color x:Key="RedGradient6">#DB3930</Color>
    <Color x:Key="RedGradient7">#C02620</Color>
