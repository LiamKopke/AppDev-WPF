    <Color x:Key="BlueGradient1">#87CEFA</Color>
    <Color x:Key="BlueGradient2">#73A5FC</Color>
    <Color x:Key="BlueGradient3">#5F7CFD</Color>
    <Color x:Key="BlueGradient4">#4B53FE</Color>
