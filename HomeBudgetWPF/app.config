<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="firstTimeUser" value="true" />
		<add key="defaultFileDirectory" value="\BudgetFiles\" />
		<add key="defaultFileName" value="budget.db" />
		<add key="lastUsedFile" value="\BudgetFiles\budget.db"/>
		<!--Use default filepath if lastUsedFilePath doesnt exist-->
		<add key="lastUsedFilePath" value="\BudgetFiles\"/>
		<!--Same logic with currentFile-->
		<add key="currentFile" value="budget"/>
		<add key="newDB" value="true"/>
		<add key="recentDB" value="false"/>
		<!-- Theme -->
		<add key="themeColor" value="Red"/>
		<add key="darkMode" value="true"/>
	</appSettings>
</configuration>
