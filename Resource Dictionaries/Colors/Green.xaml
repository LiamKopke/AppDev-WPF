<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Active colors-->
    <SolidColorBrush x:Key="App.Background" Color="{DynamicResource LightBackground}"/>
    <SolidColorBrush x:Key="MainColor" Color="{DynamicResource Green}"/>
    <SolidColorBrush x:Key="Button.OnMouseOver" Color="{DynamicResource MouseOverGreen}"/>
    <SolidColorBrush x:Key="Button.TextColor" Color="{DynamicResource White}"/>
    <SolidColorBrush x:Key="TextColor" Color="{DynamicResource Black}"/>
    <SolidColorBrush x:Key="Alternation1" Color="{DynamicResource LightAlt1}"></SolidColorBrush>
    <SolidColorBrush x:Key="Alternation2" Color="{DynamicResource LightAlt2}"></SolidColorBrush>
    <SolidColorBrush x:Key="Backdrop" Color="{DynamicResource LightBackdrop}"></SolidColorBrush>
</ResourceDictionary>